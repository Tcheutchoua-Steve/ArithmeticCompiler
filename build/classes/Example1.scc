/* define package-prefix */
Package fet.compiler;

/* define token */
Tokens
	ident = 'x';
	lpar = '(';
	rpar = ')';
	comma = ',';

Productions
	s
		= {axiom} lpar l rpar
		|{id} ident;  // name in {Â } is optional. It is the name of the node in the tree
		
	l
		= {l} l comma s
		| {s} s ;
		
